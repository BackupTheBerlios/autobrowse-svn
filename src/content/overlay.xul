				<menuitem id="ab-prefs" label="Settings" oncommand="AutoBrowse.onSettings(event);"/>
				<menuitem id="ab-prefs" label="License" oncommand="AutoBrowse.onLicense(event);"/>
			</menupopup>
		</menu>
	</menupopup>

				
</overlay>


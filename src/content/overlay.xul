<?xml-stylesheet href="chrome://autobrowse/skin/overlay.css" type="text/css"?>

<overlay id="autobrowse-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

	<script src="overlay.js"/>

	<menupopup id="menu_ToolsPopup">
		<menu label="AutoBrowse">
			<menupopup>
				<menuitem id="ab-prefs" label="Start" oncommand="AutoBrowse.onStart(event);"/>
